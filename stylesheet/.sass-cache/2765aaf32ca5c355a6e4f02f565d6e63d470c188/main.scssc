@root"X/Users/laander/Sites/projects/konsite/wp-content/themes/konscript/stylesheet/source:@template"õ /**
 * Main (main.scss)
 */

// Load the WP defaults and base
@import "_base";
@import "_wordpress";

// Default blueprint setup on the body
body.bp {
  @include blueprint-typography(true);
  @include blueprint-utilities;
  @include blueprint-debug;
  @include blueprint-interaction;
}

// Default blueprint setup for forms
form.bp {
  @include blueprint-form;
}

/**
 * 1. Body
 ************************************************/

// Contains everything
body {
	background-color: $color_secondary;
}
	#body-wrapper {
		background-color: $color_primary; // Old browsers
		@include filter-gradient($color_primary, $color_secondary, horizontal); // IE6-9 fallback on horizontal gradient
		@include background-image(radial-gradient(top, ellipse cover, $color_primary 0%, $color_secondary 50%));
		
	}

/**
 * 2. Header
 ************************************************/

// Contains entire header
#header-container {
	width: 100%;
	height: 6em;
	background-color: $color_alt-primary;
	@include filter-gradient(#151515, $color_alt-primary, horizontal); // IE6-9 fallback on horizontal gradient
	@include background-image(radial-gradient(center, ellipse cover, #151515 0%, $color_alt-primary 50%));
	//@include filter-gradient($color_alt-primary, #151515, vertical); // IE6-9
	//@include background-image(linear-gradient(top, $color_alt-primary 0%, #151515 100%));
	border-bottom: 1px solid $color_primary;
}
	#header {
		@include container;
		
		h1, div {
			font-size: 100%;
			line-height: 1;
			margin: 0;
			padding: 0;
		}
	}

		// Site title
		#site-title {
			@include column(6);
			height: 6em;				
			
			a {
				display: block;
				
				@include opacity(0.99);
				
				font-weight: 300;
				text-transform: uppercase;
				text-decoration: none;
				font-family: $font_terminal;				
				color: $color_con-primary;				

				font-size: 3em;
				
				@include replace-text("logo-optimus.png", 0%, 50%);
				background-repeat: no-repeat;
				padding: 0.5em 0;
			}
				a span {}
		}

		// Site description/tagline
		#site-description {
			display: none;
		}

	/**
	 * 2.1 Primary Menu
	 ************************************************/

	// Wrapper for entire menu area
	#primary-menu {
		@include column(18, true);
		margin: 0;
		height: 6em;
		
		// Wrapper for unordered list of menu items
		div.menu {
			margin: 0;
		}

			// Menu list.
			ul {
				margin: 0;
				padding: 0;
				float: right;
			}
				li {
					@include column(3, true);
					height: 6em;
					text-align: center;
				}
					li a {
						//@include opacity(0.99);
						display: block;
						margin: 0.6em 0;
						padding: 1.8em 0;
						//padding-left: $blueprint-grid-margin / 2;
						color: $color_secondary;
						border-left: 1px solid rgba(255,255,255,0.1);
						text-decoration: none;
						font-family: $font_terminal;
						font-size: 1.1em;
						text-transform: uppercase;
						@include transition('color', 0.5s);
						@include text-shadow($color_alt-primary 0px 2px 0px);
						&:hover {
							color: $color_con-primary;
						}
						&:active {
							margin-top: 0.65em;
						}
					}
				li.current-menu-item {
					a {
						color: $color_con-primary;
					}
				}
		}
		
/**
 * 3. Content
 ************************************************/

// Contains site's main content
#content-container {
	@include container;	
	margin-top: 2em;
}

	#content-wrapper {
		@include column(18);
		margin-bottom: 1em;
	}

	#content {
		border-left: 1px solid #DDD;	
		border-right: 1px solid #DDD;
		@include border-radius(3px);
		@include box-shadow(rgba(0,0,0,0.5) 0px 1px 0px);
		
		padding: $blueprint-grid-width + $blueprint-grid-margin 0;
		@include prepend(1);
		@include column(16);	
		@include append(1);
		
		background-color: $color_primary;
	}

		// Breadcrumb trail
		.breadcrumb {}
			.breadcrumb-trail {}

		// Archive and search <div> before posts are listed
		.archive-info
		.author-info {}
		.term-info
		.category-info {}
		.tag-info {}
		.date-info {}
		.search-info {}

			// Archive and search page titles.
			.archive-title
			.author-title {}
			.term-title {}
			.category-title {}
			.tag-title {}
			.date-title {}
			.search-title {}

		// Entry wrapper <div>
		.hentry {}
		.post {}
		.page {}

			// Post, page, and 404 page titles
			body.bp .entry-title a {
				font-family: $font_terminal;
				text-decoration: none;
				font-size: 32px;
				line-height: 1em;
			}
			.post-title {}
			.page-title {}
			.attachment-title {}
			.error-404-title {}

			// Author and date byline
			.byline {}

			// Full entries
			.entry-content {}

			// Excerpts (introduced)
			.entry-summary {}

				// Using <!--nextpage-->
				p.page-links a {}

				// More link <!--more-->
				a.more-link {}

			// Entry metadata
			.entry-meta {}

		// Post series
		.series {} 
			.series-title {}

		// Navigation links
		.navigation-links {
			.previous {}
			.next {}
		}

		// WP PageNavi (plugin)
		.wp-pagenavi {
			.pages {}
			.current {}
			.extend {}
			a {}
		}
		
		// Attachment image navigation
		.navigation-attachment {}

	/**
	 * 3.1 Comments template
	 ************************************************/

	// Wrapper for entire comments area
	#comments-template {}

		// Comments section headers/titles
		.comments-header {}
		#comments-number {}

		// Comments closed paragraph
		p.comments-closed {}

			// Main comments section
			#comments {
				ol {}

					// Individual comments (note that WordPress has even/odd backwards)
					li {}
						li li {}
							li li li {}
								li li li li {}
					li.even {}
					li.odd {}
					li.thread-even {}
					li.thread-odd {}
					li.author {}
					li.trackback {}
					li.pingback {}

						// Comment avatars
						.avatar {}

						// Comment metadata
						.comment-meta {}
							.comment-meta cite {}

						// Comment text
						.comment-text {}
			}

				// Paged comments navigation
				#comments-template .paged-navigation {}
					#comments-template .paged-navigation .page-numbers {}
					#comments-template .paged-navigation .next {}
					#comments-template .paged-navigation .prev {}

			// Main comment form div
			#respond {}

				// Comment form title
				#reply {}

				// Comment form elements
				p#cancel-comment-reply {}
				#respond form {}
					#respond .form-author {}
						#respond input {}
					#respond .form-email {}
					#respond .form-url {}
					#respond .form-textarea {}
						#respond textarea {}
					#respond #submit {}
						#respond #submit:hover {}

	/**
	 * 3.2 Widget sections (Primary and Secondary)
	 ************************************************/

	// Widgets in general
	.widget {}
		.widget-inside {}
			.widget-title {}

	// Primary widget section
	#sidebar-primary {
		@include column(6,true);

		.widget {
			padding-left: $blueprint-grid-margin;	
		}
			.widget-inside {}
				.widget-title {}
				.widget ul {}
					.widget ul li {}
	}

/**
 * 4. Footer
 ************************************************/

// Wrapper for the entire footer
#footer-container {}
	#footer {
		@include container;

		// Footer extra
		.copyright {}
		.credit {}
	}
		
/**
 * 5. Elements (default styling)
 ************************************************/

// Hyperlinks 
a:link {}
a:visited {}
a:hover {}
a:active {}

// Headers 
h1, h2, h3, h4, h5, h6 { text-transform: uppercase; }
h1 {}
